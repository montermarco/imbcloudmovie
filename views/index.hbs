<h1>Bienvenido al proyecto Cinefilo</h1>

<div class="movies-wrapper">
  {{#each movies}}
    <div class="movie-item">
      <img src="{{this.imgPath}}" alt="">
      <h4>{{this.title}}</h4>
      <p>{{this.description}}</p>
     </div>
  {{else}}
    <p>No hay peliculas aun valedor!</p>
  {{/each}}
</div>

